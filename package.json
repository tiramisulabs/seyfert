{
	"name": "seyfert",
	"version": "1.5.0",
	"description": "The most advanced framework for discord bots",
	"main": "./lib/index.js",
	"module": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"lib/**"
	],
	"scripts": {
		"build": "tsc --outDir ./lib",
		"prepublishOnly": "npm run build",
		"prepare": "npm run build && husky",
		"lint": "biome lint --write ./src",
		"format": "biome format --write ./src",
		"check-h": "biome check --write ./src",
		"check": "biome check --write --no-errors-on-unmatched ./src"
	},
	"author": "MARCROCK22",
	"license": "Apache-2.0",
	"dependencies": {
		"chokidar": "^3.6.0",
		"discord-api-types": "^0.37.90",
		"magic-bytes.js": "^1.10.0",
		"ts-mixer": "^6.0.4",
		"ws": "^8.17.1"
	},
	"lint-staged": {
		"*.ts": [
			"biome check --write"
		]
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.1",
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@types/node": "^20.14.7",
		"@types/ws": "^8.5.10",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.7",
		"typescript": "^5.5.2"
	},
	"optionalDependencies": {
		"ioredis": "^5.4.1",
		"tweetnacl": "^1.0.3",
		"uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.42.0"
	},
	"homepage": "https://seyfert.dev",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tiramisulabs/seyfert.git"
	},
	"bugs": {
		"url": "https://github.com/tiramisulabs/seyfert"
	},
	"keywords": [
		"api",
		"discord",
		"bots",
		"typescript",
		"botdev"
	],
	"publishConfig": {
		"access": "public"
	},
	"contributors": [
		{
			"name": "socram03",
			"url": "https://github.com/socram03"
		}
	]
